{
  "name": "@obsidian-google-drive-sync/openapi-schema",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm run /^.*:compile:watch$/",
    "tsp:compile:watch": "tsp compile src/main.tsp --watch",
    "tsp:format": "tsp format src/main.tsp",
    "orval:compile:watch": "orval --watch",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "@typespec/http": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "ky": "^1.14.0"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@typespec/compiler": "latest",
    "orval": "^7.16.1"
  }
}
